{
  "name": "cardinal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:fix": "eslint --fix",
    "prepare": "husky install",
    "format:fix": "prettier --write",
    "test": "jest --detectOpenHandles --runInBand",
    "typecheck": "tsc",
    "test:cov": "jest --coverage",
    "format": "prettier --write \"**/*.{json,md,ts,tsx}\" \"*.{json,md,ts,tsx}\"",
    "format-verify": "prettier --check \"**/*.{json,md,ts,tsx}\" \"*.{json,md,ts,tsx}\"",
    "lint": "eslint --ext ts,tsx",
    "verify": "npm run typecheck && npm run lint && npm run format-verify && npm run test"
  },
  "dependencies": {
    "@gothicgeeks/design-system": "git+ssh://git@github.com:thrownullexception/design-system.git#ebe833507bdf6541787ef0c5b30a723f8448bc46",
    "@gothicgeeks/shared": "git+ssh://git@github.com:thrownullexception/shared.git#566b79f9104b0e6fedbc266b6ca063e5ed375f39",
    "ajv": "^8.11.0",
    "change-case": "^4.1.2",
    "class-validator": "^0.13.2",
    "final-form-arrays": "^3.0.2",
    "fs-extra": "^10.1.0",
    "knex": "^2.0.0",
    "mustache": "^4.2.0",
    "next": "12.1.6",
    "pg": "^8.7.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-feather": "^2.0.10",
    "react-final-form-arrays": "^3.1.3",
    "react-query": "^3.39.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.9",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
